<?xml version="1.0" encoding="ISO-8859-1"?>
<mule xmlns="http://www.mulesource.org/schema/mule/core/2.2"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:spring="http://www.springframework.org/schema/beans"
       xmlns:file="http://www.mulesource.org/schema/mule/file/2.2"
       xmlns:tcp="http://www.mulesource.org/schema/mule/tcp/2.2"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
           http://www.mulesource.org/schema/mule/file/2.2 http://www.mulesource.org/schema/mule/file/2.2/mule-file.xsd
           http://www.mulesource.org/schema/mule/tcp/2.2 http://www.mulesource.org/schema/mule/tcp/2.2/mule-tcp.xsd">

    <model>
        <service name="TcpReceiver">
            <inbound>
                <tcp:inbound-endpoint host="localhost" port="4444"/>            
                <forwarding-router/>
            </inbound>
            <null-component/>
            <outbound>
                <pass-through-router>
                    <file:outbound-endpoint path="temp/tests/mule"/>
                </pass-through-router>
            </outbound>
        </service>    
    </model>
</mule>
